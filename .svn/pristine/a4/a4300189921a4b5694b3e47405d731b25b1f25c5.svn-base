.radio, .checkbox {
    position: relative;
    display: block;
}
.radio label, .checkbox label {
    padding-left: 20px;
    padding-right:20px;
    margin-bottom: 0;
    font-weight: normal;
    cursor: pointer;
}
.radio input[type="radio"], .radio-inline input[type="radio"], .checkbox input[type="checkbox"], .checkbox-inline input[type="checkbox"] {
    position: absolute;
    margin-top: 2px;
    margin-left: -20px;
}


INPUT[type="submit"].gray, 
INPUT[type="button"].gray,
BUTTON.gray,
A.gray {
	color:#fff;
	border:1px solid #535b60;
	background-color:#535b60;
}

INPUT[type="submit"].white, 
INPUT[type="button"].white,
BUTTON.white,
A.white {
	color:#5c5c5c;
	border:1px solid #b5b6b3;
	background-color:#fff;
}

INPUT[type="submit"].disable, 
INPUT[type="button"].disable,
BUTTON.disable,
A.disable {
	border:1px solid #eee;
	background-color:#f7f8f9;
	cursor:normal;
	color:#ccc;
}
INPUT[type="submit"].tiny, 
INPUT[type="button"].tiny,
BUTTON.tiny,
A.tiny {
	font-size:12px;
	padding:0px 2px;
	height:19px;
}


/* Combo Box */
SELECT, SELECT:hover, SELECT:focus, SELECT:active {
	padding:5px 5px 5px 3px;
	margin-right:3px;
	border:1px solid #b5b6b3;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	-khtml-border-radius:5px;
	border-radius:5px;
	color:#8a8a8a;
	font-size:12px;
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
}

SELECT.square, SELECT.square:hover, SELECT.square:focus, SELECT.square:active {
	padding:4px 4px 4px 4px;
	margin-right:3px;
	border:1px solid #b5b6b3;
	-moz-border-radius:0px;
	-webkit-border-radius:0px;
	-khtml-border-radius:0px;
	border-radius:0px;
	background:#eee;
	color:#8a8a8a;
	font-size:13px;
	font-family:"맑은 고딕"
	-moz-box-shadow: inset 0 0 10px #e9e9e9;
	-webkit-box-shadow: inset 0 0 10px #e9e9e9;
	box-shadow: inset  0 0 10px #e9e9e9;
	box-sizing:content-box;
	-ms-box-sizing:content-box;
	-moz-box-sizing:content-box;
	-webkit-box-sizing:content-box; 
}

.pagelist {
	margin:20px;
	text-align:center;
}
.pagelist button {
	margin:0;
	padding:0 6px;
	height:32px;
	cursor:pointer;
	vertical-align:top;
 	border-radius: 20px;
	
}
.pagelist button:hover {
	background:#fff;
	border:1px solid #c4c7d0;
}
.pagelist button.pageBtn {
	font-size:13px;
	color:#aaa;
	border:1px solid #f0f2f3;
}
.pagelist button.pageBtnCurrent {
	font-size:13px;
	color:#77f;
	border:1px solid #f0f2f3;
}

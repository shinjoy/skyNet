	fileupload :function(){
		var options = {
				//target :		'#user-join-result',
				beforeSubmit :	formCheck,
				success :		formSuccess
		};
		$('#dataForm').ajaxForm(options);
	},
	formCheck : function() {
		var str = '<li class="upload-progress"><img src="/images/ajax-loader.gif"><li>';
		$(".file-add").append(str);
		return true; 
	},
	formSuccess : function (responseText, statusText, xhr, $form) {
		//alert('status: ' + statusText + '\n\nresponseText: \n' + responseText );
	
		var json = JSON.parse(responseText);
		try {
			$(".upload-progress").remove();
			
			if(json.photo!=""){
				$("#img_1").empty();
				var str = '';
				var fileName='/files'+json.path +'/'+ json.fileName;
				str +=' <div class="photo-detail" style="background-image:url('+fileName+');"></div>';
				
				str += '	<button type="button" class="del-btn" onclick="projectAdmin.fileDelete(\''+ json.path +'/'+ json.fileName +'\',1)">X</button>';
			
				
				$("#img_1").append(str);
				$("#fileName").val('/files'+json.path +'/'+ json.fileName);
				
			}
		} catch (e) {
			pop.openAlert('',json.message); 
		}
	},
	fileDelete : function(fileName,num){
		
		$("#img_"+num).empty();
		$("#fileName").val("");
					
	},
